    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "conf": "^1.2.0",
    "figlet": "^1.2.0",
    "fs-extra": "^2.1.2",
    "iconv-lite": "^0.4.15",
    "inquirer": "^3.0.6",
    "is-root": "^1.0.0",
    "ora": "^1.2.0",
    "qrcode-terminal": "^0.12.0",
    "query-string": "^5.0.0",
